<script>
  $(".c1").click(function () {
    if ($("input[name=checkListClub]").val().trim() == "") {
      return true;
    }
    var $toAdd = $("input[name=checkListClub]").val();
    
    if(!isValidHttpUrl($toAdd)){
      $('#note2').text('Only URL allowed, URL must contain http or https')
      return true;
    }else{
      $('#note2').text('')
    }
    $(".list").append(
      '<div class="item"><div class="item-close"><a target="_blank" href="'+$toAdd+'">' + $toAdd + "</a><i class='fa-solid fa-xmark ms-3'></i></div></div>"
    );
    var $toAdd = $("input[name=checkListClub]").val("");
  });
  $(document).on("click", ".item .fa-solid", function () {
    $(this).parent().remove();
  });

  $(".c2").click(function () {
    if ($("input[name=checkListEvents]").val().trim() == "") {
      return true;
    }
    var $toAddmore = $("input[name=checkListEvents]").val();
    if(isValidHttpUrl($toAddmore)){
      $toAddmore=`<a href="${$toAddmore}" target="_blank">${$toAddmore}</a>`;
    }
    $(".list-2").append(
      '<div class="item-2"><div class="item-close">' +
        $toAddmore +
        "<i class='fa-solid fa-xmark ms-3'></i></div></div>"
    );
    $("input[name=checkListEvents]").val("");
  });
  $(document).on("click", ".item-2 .fa-solid", function () {
    $(this).parent().remove();
  });

  $(".c3").click(function () {
    if ($("input[name=RecidenceClub]").val().trim() == "") {
      return true;
    }
    var $toRefrence = $("input[name=RecidenceClub]").val();
    $(".list-3").append(
      '<div class="item-3"><div class="item-close">' +
        $toRefrence +
        "</div></div>"
    );
    $("input[name=RecidenceClub]").val("");
  });
  function isValidHttpUrl(string) {
    let url;
    
    try {
      url = new URL(string);
    } catch (_) {
      return false;  
    }

    return url.protocol === "http:" || url.protocol === "https:";
  }
  $(document).on("click", ".item-3", function () {
    $(this).remove();
  });
</script>

<script>
  $(document).ready(function () {
    $("input[name='club-1']:radio").change(function () {
      if ($(this).val() == "c1") {
        $(".list-2 .item-close").remove();
        $(".club-1").show();
        $(".club-2").hide();
      } else if ($(this).val() == "c2") {
        $(".list .item-close").remove();
        $(".club-2").show();
        $(".club-1").hide();
      } else if ($(this).val() == "c3") {
        $(".club-2").show();
        $(".club-1").show();
      }
    });
  });

  $(document).ready(function () {
    $('#checkAll').click(function () {
        $('input[name="music"]').prop('checked', this.checked);
    });
    const radio = $('input[name="club-1"]:checked').val();
    if (radio == "c1") {
      $(".club-1").show();
      $(".club-2").hide();
    } else if (radio == "c2") {
      $(".club-1").hide();
      $(".club-2").show();
    } else {
      $(".club-1").show();
      $(".club-2").show();
    }
  });
</script>
