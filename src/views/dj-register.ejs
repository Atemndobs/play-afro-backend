<%- include('layouts/common_header') %>
<style>
  #dj-register label.form-label b {
    color: #de0000;
  }
  #company_register label.form-label b {
    color: #de0000;
  }

  .refrence-cover .form-check label { 
    word-wrap: break-word !important;
    display: inline !important;
}
  .form-check label {
    word-wrap: break-word !important;
    display: block;
  }
  .form-add-delete {
    border: 1px solid #ced4da;
    border-radius: 8px;
    padding: 10px 25px;
    background: #fff;
  }

  .form-add-delete input {
    border: none;
    background: transparent;
    outline: none;
  }

  .form-add-delete button {
    border-radius: 8px;
    padding: 5px 10px;
    background: transparent;
    border: 1px solid #001ef0;
    color: #001ef0;
  }

  .events-clubs .list .item .item-close,
  .events-clubs .list-2 .item-2 .item-close,
  .events-clubs .list-3 .item-3 .item-close {
    background: linear-gradient(98.88deg, #4157e9 0%, #6d11b8 100%);
    border-radius: 8px;
    padding: 5px 10px;
    color: #fff;
    margin-right: 10px;
  }
  .item-close a{
    color: #fff !important;
  }

  .list-3,
  .list-2,
  .list {
    flex-wrap: wrap;
    margin-bottom: 15px;
  }

  .club-3 .item-close:after {
    background: transparent;
    color: #fff;
    border-radius: 10px;
    content: "X";
    padding: 5px;
    font-size: 15px;
    margin-left: 10px;
    margin-bottom: 15px;
  }
</style>

<div class="register-form" id="register_form">
